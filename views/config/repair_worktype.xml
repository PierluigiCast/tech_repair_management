<odoo>
    <record id="view_tech_repair_worktype_form" model="ir.ui.view">
        <field name="name">tech.repair.worktype.form</field>
        <field name="model">tech.repair.worktype</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="price"/>
                        <field name="stimated_time"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tech_repair_worktype_list" model="ir.ui.view">
        <field name="name">tech.repair.worktype.list</field>
        <field name="model">tech.repair.worktype</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="description"/>
                <field name="price"/>
                <field name="stimated_time"/>
            </list>
        </field>
    </record>

    <record id="action_tech_repair_worktype" model="ir.actions.act_window">
        <field name="name">Categorie Dispositivi</field>
        <field name="res_model">tech.repair.worktype</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
