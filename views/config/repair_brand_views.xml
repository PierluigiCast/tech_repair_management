<!-- Interfaccia gestione brand -->
<odoo>
    <record id="view_tech_repair_brand_form" model="ir.ui.view">
        <field name="name">tech.repair.brand.form</field>
        <field name="model">tech.repair.brand</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tech_repair_brand_list" model="ir.ui.view">
        <field name="name">tech.repair.brand.list</field>
        <field name="model">tech.repair.brand</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="action_tech_repair_brand" model="ir.actions.act_window">
        <field name="name">Marche Dispositivi</field>
        <field name="res_model">tech.repair.brand</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
